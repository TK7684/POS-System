# Firebase to Supabase Migration - Environment Variables Template
# Copy this file to .env and fill in your actual values

# ====================================================================
# SUPABASE CONFIGURATION
# ====================================================================
# Get these from your Supabase project: Settings > API
SUPABASE_URL=https://your-project-ref.supabase.co
SUPABASE_SERVICE_ROLE_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...your-service-role-key
SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...your-anon-key
SUPABASE_PROJECT_REF=your-project-ref

# ====================================================================
# FIREBASE CONFIGURATION (for data export)
# ====================================================================
# Your Firebase project ID
FIREBASE_PROJECT_ID=pos-agent-8767b

# Path to Firebase service account key (download from Firebase Console)
# See: https://firebase.google.com/docs/admin/setup#initialize_the_sdk
FIREBASE_SERVICE_ACCOUNT_KEY=./serviceAccountKey.json

# ====================================================================
# DATABASE CONFIGURATION
# ====================================================================
# Direct database connection (optional, for psql operations)
# Format: postgresql://postgres:[password]@db.[project-ref].supabase.co:5432/postgres
SUPABASE_DATABASE_URL=postgresql://postgres:your-password@db.your-project-ref.supabase.co:5432/postgres

# ====================================================================
# MIGRATION SETTINGS
# ====================================================================
# Number of records to process in each batch
MIGRATION_BATCH_SIZE=100

# Maximum retry attempts for failed operations
MIGRATION_MAX_RETRIES=3

# Delay between batches (milliseconds)
MIGRATION_BATCH_DELAY=100

# Skip existing records during import (true/false)
MIGRATION_SKIP_EXISTING=true

# Enable verbose logging (true/false)
MIGRATION_DEBUG=false

# Continue migration on errors (true/false)
MIGRATION_CONTINUE_ON_ERROR=true

# ====================================================================
# AUTHENTICATION CONFIGURATION
# ====================================================================
# Google OAuth (for Supabase Auth)
# Get these from Google Cloud Console: APIs & Services > Credentials
GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-google-client-secret

# Redirect URLs for OAuth
AUTH_REDIRECT_URL=http://localhost:3000/auth/callback
AUTH_SITE_URL=http://localhost:3000

# ====================================================================
# INTEGRATION CONFIGURATION
# ====================================================================
# Google Sheets API (if using spreadsheet integration)
GOOGLE_SHEETS_API_KEY=AIzaSy...your-google-sheets-api-key
GOOGLE_SHEETS_CLIENT_ID=your-google-sheets-client-id.apps.googleusercontent.com
GOOGLE_SHEETS_SPREADSHEET_ID=your-spreadsheet-id

# Line Bot (if using Line integration)
LINE_CHANNEL_ACCESS_TOKEN=your-line-channel-access-token
LINE_CHANNEL_SECRET=your-line-channel-secret
LINE_GROUP_ID=your-line-group-id

# ====================================================================
# HOSTING CONFIGURATION
# ====================================================================
# Netlify (if deploying to Netlify)
NETLIFY_SITE_ID=your-netlify-site-id
NETLIFY_AUTH_TOKEN=your-netlify-auth-token

# Vercel (if deploying to Vercel)
VERCEL_ORG_ID=your-vercel-org-id
VERCEL_PROJECT_ID=your-vercel-project-id
VERCEL_TOKEN=your-vercel-token

# Custom domain (if using)
SITE_DOMAIN=your-domain.com

# ====================================================================
# DEVELOPMENT CONFIGURATION
# ====================================================================
# Environment (development/staging/production)
NODE_ENV=development

# Port for local development
PORT=3000

# Enable CORS for development
DEV_CORS_ORIGIN=http://localhost:3000

# ====================================================================
# FEATURE FLAGS
# ====================================================================
# Enable/disable features during migration
ENABLE_REAL_TIME_SUBSCRIPTIONS=true
ENABLE_GOOGLE_AUTH=true
ENABLE_EMAIL_AUTH=true
ENABLE_GOOGLE_SHEETS=true
ENABLE_LINE_BOT=true
ENABLE_ANALYTICS=true

# ====================================================================
# SECURITY CONFIGURATION
# ====================================================================
# JWT Secret for custom authentication (if needed)
JWT_SECRET=your-super-secret-jwt-key-here

# Encryption key for sensitive data (if needed)
ENCRYPTION_KEY=your-32-character-encryption-key

# ====================================================================
# LOGGING CONFIGURATION
# ====================================================================
# Log level (error, warn, info, debug)
LOG_LEVEL=info

# Log file path (optional)
LOG_FILE=./logs/migration.log

# ====================================================================
# PERFORMANCE CONFIGURATION
# ====================================================================
# Connection pool size for database operations
DB_POOL_SIZE=10

# Timeout for database operations (milliseconds)
DB_TIMEOUT=30000

# Maximum concurrent requests
MAX_CONCURRENT_REQUESTS=10

# ====================================================================
# BACKUP CONFIGURATION
# ====================================================================
# Backup directory path
BACKUP_DIR=./backups

# Include Firebase backup (true/false)
BACKUP_FIREBASE=true

# Include Supabase backup (true/false)
BACKUP_SUPABASE=true

# Backup retention (days)
BACKUP_RETENTION_DAYS=30

# ====================================================================
# TESTING CONFIGURATION
# ====================================================================
# Run tests after migration (true/false)
RUN_POST_MIGRATION_TESTS=true

# Test data file (optional)
TEST_DATA_FILE=./test-data.json

# Validation rules file (optional)
VALIDATION_RULES_FILE=./validation-rules.json

# ====================================================================
# MONITORING CONFIGURATION
# ====================================================================
# Performance monitoring (if using external service)
PERFORMANCE_MONITORING_URL=https://your-monitoring-service.com/webhook

# Error reporting service
ERROR_REPORTING_ENDPOINT=https://your-error-reporting.com/webhook

# Health check interval (seconds)
HEALTH_CHECK_INTERVAL=60

# ====================================================================
# NOTES
# ====================================================================
# 1. Never commit this file with real credentials to version control
# 2. Keep service role keys and secrets secure
# 3. Use different values for development, staging, and production
# 4. Update redirect URLs after deployment
# 5. Regularly rotate secrets and keys
# 6. Test all integrations after updating environment variables

